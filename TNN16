    <script>
      //lazy load
      if ('loading' in HTMLImageElement.prototype) {
        const images = document.querySelectorAll("picture.img.lazyload");
        images.forEach(img => {
          img.src = img.dataset.src;
        });
      } else {
        let script = document.createElement("script");
        script.async = true;
        script.src = "https://cdnjs.cloudflare.com/ajax/libs/lazysizes/4.1.8/lazysizes.min.js";
        document
          .body
          .appendChild(script);
      }
      //sticky
      var lastKnownScrollY = 0;
      var currentScrollY = 0;
      var CheckHeader = 'menubar-sticky';
      var TopHeader = null;
      const classes = {
        pinned: '--sticky-show',
        unpinned: '--sticky-hide'
      };
      function onScroll() {
        currentScrollY = window.pageYOffset;
        if (currentScrollY < lastKnownScrollY) {
          pin();
        } else if (currentScrollY > lastKnownScrollY) {
          unpin();
        }
        lastKnownScrollY = currentScrollY;
      }
      function pin() {
        if (TopHeader.classList.contains(classes.unpinned)) {
          TopHeader
            .classList
            .remove(classes.unpinned);
          TopHeader
            .classList
            .add(classes.pinned);
        }
      }
      function unpin() {
        if (TopHeader.classList.contains(classes.pinned) || !TopHeader.classList.contains(classes.unpinned)) {
          TopHeader
            .classList
            .remove(classes.pinned);
          TopHeader
            .classList
            .add(classes.unpinned);
        }
      }
      window.onload = function () {
        TopHeader = document.getElementById(CheckHeader);
        document.addEventListener('scroll', onScroll, false);
      };
      //menu
      function toggleMenu() {
        document
          .getElementById("submenu")
          .classList
          .toggle("block");
        var myButtonClasses = document
          .getElementById("btn-chenel")
          .classList;
        if (myButtonClasses.contains("onn")) {
          myButtonClasses.remove("onn");
        } else {
          myButtonClasses.add("onn");
        }
      }
      //toggle search
      function toggleSearch() {
        document
          .getElementById('tnnsearch')
          .classList
          .toggle('show');
        var myButtonClasses = document
          .querySelector('#btn-search')
          .classList;
        if (myButtonClasses.contains("close-search")) {
          myButtonClasses.remove("close-search");
        } else {
          myButtonClasses.add("close-search");
        }
      }
    </script>
    <script>
      function formatDate(date) {
        let month = '' + (
        date.getMonth() + 1)
        let day = '' + date.getDate()
        let year = date.getFullYear()

        if (month.length < 2) {
          month = '0' + month
        }
        if (day.length < 2) {
          day = '0' + day
        }

        return `${year}${month}${day}`
      }
      function isCurrentProgram(start, end, m_now) {
        let m_start = moment().format(start, "YYYY-MM-DD HH:mm:ss")
        let m_end = moment().format(end, "YYYY-MM-DD HH:mm:ss")
        let is_between = m_now.isBetween(m_start, m_end)
        let is_visible = m_now.isBefore(m_end)
        return {is_between, is_visible}
      }
      const today = new Date()

      let date_param = formatDate(today)

      function fetchTvPlan(date_param) {
        let api = `https://mamapi.stm.trueid.net/epg/stream/135/${date_param}/${date_param}`
        var xhttp = new XMLHttpRequest();
        let tvplan_body = document.getElementById('tvplan-body')
        xhttp.onreadystatechange = function () {
          if (this.readyState === 4 && this.status === 200) {
            let data = JSON.parse(this.responseText)
            let result = data.result
            let m_now = moment()
            if (result.schedules) {
              let list = result.schedules
              let count = 0
              let next_plan = document.getElementById('next-plan');
              for (let i = 0; i < result.schedules.length; i++) {
                let plan = result.schedules[i]
                let current_program = isCurrentProgram(plan.start, plan.end, m_now)
                if (current_program.is_between) {
                  let live_title = document.getElementById('live-title');
                  live_title.innerHTML = `${plan.title_th} (${getPlanTime(plan.start)} - ${getPlanTime(plan.end)} น.)`
                } else if (current_program.is_visible) {
                  let plan_row = {
                    title: plan.title_th,
                    time: `${getPlanTime(plan.start)} - ${getPlanTime(plan.end)}`
                  }

                  let li = makePlan(plan_row, count === 0);
                  next_plan.appendChild(li);
                  count++
                }
                if (count >= 3) {
                  let li = document.createElement('li');
                  li.innerHTML = `
                <a href="/tvplan/">ดูทั้งหมด</a>
              `
                  next_plan.appendChild(li);
                  break;
                }

              }
            }
          }
        }
        xhttp.open("GET", api, true);
        xhttp.setRequestHeader("token", "dee2b109-7103-4ee4-88a9-180ef9155f81");
        xhttp.send();
      }

      fetchTvPlan(date_param)

      function makePlan(plan, is_next) {
        let li = document.createElement('li');
        li.className = is_next
          ? 'next'
          : '';
        li.innerHTML = `
      <p>${plan.time}</p>
      <h3>${plan.title}</h3>
    `
        return li
      }

      function getPlanTime(date) {
        let time = date.split(" ")[1]
        return time.slice(0, 5)
      }
    </script>
