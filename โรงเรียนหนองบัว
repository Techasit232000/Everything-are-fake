        jQuery(document).ready(function ($) {
            var options = {
                $AutoPlay: 1,                                       
                $AutoPlaySteps: 1,                                  
                $Idle: 2000,                                        
                $PauseOnHover: 1,                                   
                $ArrowKeyNavigation: 1,   			                
